if [ $machinetyp = "x86_64" ] ; then
	exit 0
else
	echo "Dein Ger채t (${device}) mit der CPU-Architektur ${machinetyp} wird von synOCR nicht unterst체tzt (nur x86_64)" > $SYNOPKG_TEMP_LOGFILE
